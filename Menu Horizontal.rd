<style>
body {
            margin: 0;
            padding: 0;
        }

        /* Estilo para a barra de banner */
        .banner-bar {
            background-color: #f2f2f260;
            text-align: center;
            padding: 20px 0;
        }

        /* Estilo par+a o banner */
        .banner {
            max-width: 97%;
            height: auto;
            text-align: center;
        }

        /* Estilo para o título dentro do banner */
        .banner h1 {
            margin: 0;
            color: #ffffff00;
        }

        /* Estilo para a subdivisão abaixo do título */
        .banner hr {
            margin: 10px 0;
            border: 0;
            height: 1px;
            background-color: #cccccc; /* Cinza claro */
        }

        /* Estilo para a barra de menu */
        .menu-bar {
            background-color: transparent;
            display: flex;
            justify-content: center;
            align-items: center;
            height: 50px;
        }

        .banner-bar img {
            height: 70px;




        }

        /* Estilo para os itens do menu */
        .menu-item {
            padding: 10px 20px;
            color: black;
            text-decoration: none;
            transition: background-color 0.3s ease;
        }

        /* Estilo para os itens do menu ao passar o mouse */
        .menu-item:hover {
            background-color: #cccccc00;
            color: rgb(0, 110, 255);
        }</style>
